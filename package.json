{
  "name": "wetools",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "postinstall": "husky install",
    "format": "lint-staged",
    "watch": "pnpm --filter './packages' --parallel run start",
    "watch:all": "pnpm --filter './packages/**' --parallel run start",
    "dev": "pnpm --filter=@wetools/gh-pages run dev",
    "start": "concurrently -n watch,doc \"pnpm watch\" \"pnpm dev\"",
    "start:all": "concurrently -n watch,dev \"pnpm watch:all\" \"pnpm dev\"",
    "start:dev": "pnpm --filter=@wetools/gh-pages run start",
    "test": "pnpm test:lint && pnpm test:unit",
    "test:unit": "vitest",
    "test:lint": "eslint \"**/{src,pages,packs}/**/*.{js,ts,tsx}\"",
    "build:packs": "pnpm --filter './packages/**' run build",
    "build:dev": "pnpm --filter=@wetools/gh-pages run build",
    "build:cache": "nx run-many --target=build --all --parallel",
    "build:affected": "nx affected --target=build",
    "graph": "nx dep-graph",
    "clear": "rimraf '**/{lib,tsconfig.tsbuildinfo,node_modules,.rollup.cache}' && rimraf node_modules",
    "changeset": "changeset",
    "release": "changeset publish",
    "commit": "git-cz"
  },
  "devDependencies": {
    "@changesets/cli": "^2.22.0",
    "@commitlint/cli": "^17.0.1",
    "@commitlint/config-conventional": "^17.0.0",
    "@nrwl/cli": "latest",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/tao": "latest",
    "@nrwl/workspace": "latest",
    "@types/node": "^17.0.36",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.5",
    "@types/rollup-plugin-auto-external": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "@vitejs/plugin-react": "^1.3.2",
    "changeset": "^0.2.6",
    "classnames": "^2.3.1",
    "concurrently": "^7.2.1",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-tsdoc": "^0.2.16",
    "git-cz": "^4.9.0",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.3",
    "nx": "14.1.9",
    "prettier": "^2.6.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.75.3",
    "rollup-plugin-auto-external": "^2.0.0",
    "typescript": "^4.7.2",
    "vite": "^2.9.9",
    "vitest": "^0.13.0"
  }
}
